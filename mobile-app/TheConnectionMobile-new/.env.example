# The Connection Mobile App Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# API Configuration
API_URL=https://api.theconnection.app/api

# Giphy API Key (get from https://developers.giphy.com/)
GIPHY_API_KEY=your_giphy_api_key_here

# Sentry DSN (get from https://sentry.io/)
SENTRY_DSN=your_sentry_dsn_here

# App Environment (production or development)
APP_ENV=production
