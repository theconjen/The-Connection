{
  "name": "theconnectionmobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios-local": "expo prebuild --platform ios && pnpm run postbuild && (cd ios && xcodebuild -workspace TheConnectionMobile.xcworkspace -scheme TheConnectionMobile -configuration Release -destination 'generic/platform=iOS')",
    "prebuild": "expo prebuild --clean && pnpm run postbuild",
    "postbuild": "node scripts/fix-expo-objc-imports.js",
    "sync:assets": "bash ./scripts/sync-web-assets.sh",
    "eas-build-pre-install": "npm install -g pnpm@10.16.1 && pnpm --version"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/merriweather": "^0.2.3",
    "@expo-google-fonts/playfair-display": "^0.2.3",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "@tanstack/react-query": "^5.90.10",
    "@types/react": "~18.3.12",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.12",
    "expo-document-picker": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "29.0.13",
    "lucide-react-native": "^0.460.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-native": "0.76.6",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-reanimated": "~3.16.5",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-share": "^11.0.4",
    "react-native-svg": "15.8.0",
    "tailwindcss": "^3.4.18"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@react-native-community/cli": "^15.1.3",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-native-picker/picker": "^2.9.0",
    "@react-native/metro-config": "0.76.6",
    "@types/node": "^22.10.1",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-preset-expo": "~12.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-native": "^4.1.0",
    "metro": "^0.81.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "private": true
}
