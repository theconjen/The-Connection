{
  "cli": {
    "version": ">= 16.26.0"
  },
  "build": {
    "production": {
      "node": "22.11.0",
      "pnpm": "10.19.0",
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_API_BASE": "https://api.theconnection.app/api"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "node": "22.11.0",
      "pnpm": "10.19.0",
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE": "https://api-preview.theconnection.app/api"
      }
    },
    "development": {
      "node": "22.11.0",
      "pnpm": "10.19.0",
      "channel": "development",
      "developmentClient": true,
      "env": {
        "EXPO_PUBLIC_API_BASE": "http://localhost:3000/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
