{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": {
        "version": "22.0.0",
        "packageManager": "pnpm"
      },
      "pnpm": {
        "installArgs": ["--frozen-lockfile"]
      }
    },
    "preview": {
      "distribution": "internal",
      "node": {
        "version": "22.0.0",
        "packageManager": "pnpm"
      },
      "pnpm": {
        "installArgs": ["--frozen-lockfile"]
      }
    },
    "production": {
      "autoIncrement": true,
      "node": {
        "version": "22.0.0",
        "packageManager": "pnpm"
      },
      "pnpm": {
        "installArgs": ["--frozen-lockfile"]
      }
    }
  },
  "submit": {
    "production": {}
  }
}
