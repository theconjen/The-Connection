{
	"cli": {
		"version": ">= 16.26.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"node": "22.0.0",
			"pnpm": "10.16.1",
			"env": {
				"EAS_PROJECT_ROOT": "mobile-app/TheConnectionMobile",
				"EXPO_PUBLIC_API_BASE": "http://localhost:3000/api"
			}
		},
		"preview": {
			"distribution": "internal",
			"node": "22.0.0",
			"pnpm": "10.16.1",
			"env": {
				"EAS_PROJECT_ROOT": "mobile-app/TheConnectionMobile",
				"EXPO_PUBLIC_API_BASE": "https://api-preview.theconnection.app"
			}
		},
		"production": {
			"autoIncrement": true,
			"node": "22.0.0",
			"pnpm": "10.16.1",
			"env": {
				"EAS_PROJECT_ROOT": "mobile-app/TheConnectionMobile",
				"EXPO_PUBLIC_API_BASE": "https://api.theconnection.app"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
