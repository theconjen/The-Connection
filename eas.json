{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "development",
      "prebuildCommand": "bash hooks/eas-build-pre-install.sh"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "preview",
      "prebuildCommand": "bash hooks/eas-build-pre-install.sh"
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "prebuildCommand": "bash hooks/eas-build-pre-install.sh"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "janelleselou2@yahoo.com",
        "ascAppId": "6755203547",
        "appleTeamId": "AN976FH68W"
      },
      "android": {
        "serviceAccountKeyPath": "./path-to-service-account.json",
        "track": "internal"
      }
    }
  }
}
